{
  "name": "react-naver-maps-kit",
  "version": "1.3.0",
  "description": "Modern React toolkit for Naver Maps",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && rolldown -c && tsc -p tsconfig.build.json && cp src/types/naver-maps-extensions.d.ts dist/ && echo '/// <reference path=\"./naver-maps-extensions.d.ts\" />' | cat - dist/index.d.ts > temp && mv temp dist/index.d.ts",
    "dev": "rolldown -c --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "e2e:marker": "playwright test marker.spec.ts",
    "e2e:ci": "CI=1 playwright test",
    "e2e:install": "playwright install chromium",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "react",
    "naver",
    "naver-maps",
    "naver-map",
    "maps",
    "map",
    "korea",
    "korean-map",
    "react-component",
    "typescript",
    "naver-cloud-platform"
  ],
  "author": "cobocho <contact@un-defined.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cobocho/react-naver-maps-kit.git"
  },
  "homepage": "https://github.com/cobocho/react-naver-maps-kit#readme",
  "bugs": {
    "url": "https://github.com/cobocho/react-naver-maps-kit/issues"
  },
  "packageManager": "pnpm@10.4.1",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.49.0",
    "@testing-library/react": "^16.2.0",
    "@types/navermaps": "^3.9.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supercluster": "^7.1.3",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^17.3.1",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rolldown": "^1.0.0-beta.41",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "supercluster": "^8.0.1"
  }
}
