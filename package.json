{
  "name": "react-naver-maps-kit-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "build:lib": "pnpm --filter react-naver-maps-kit build",
    "build:docs": "pnpm build:lib && pnpm --filter react-naver-maps-kit-docs build",
    "build:playground": "pnpm build:lib && pnpm --filter playground build",
    "docs:build": "pnpm build:docs",
    "playground:build": "pnpm build:playground",
    "dev": "pnpm --filter playground dev",
    "dev:docs": "pnpm --filter react-naver-maps-kit-docs dev"
  },
  "devDependencies": {}
}
