{
  "name": "react-naver-maps-kit",
  "version": "0.0.1",
  "description": "Modern React toolkit for Naver Maps",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && rolldown -c && tsc -p tsconfig.build.json",
    "dev": "rolldown -c --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "keywords": [
    "react",
    "naver-map",
    "maps",
    "library"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/navermaps": "^3.9.1",
    "@eslint/js": "^9.21.0",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.2",
    "rolldown": "^1.0.0-beta.41",
    "@testing-library/react": "^16.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vitest": "^2.1.8"
  }
}
