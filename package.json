{
  "name": "react-naver-maps-kit-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Modern React toolkit for Naver Maps - Monorepo",
  "type": "module",
  "scripts": {
    "build": "pnpm --filter @cobocho/react-naver-maps-kit build",
    "dev": "pnpm --filter @cobocho/react-naver-maps-kit dev",
    "test": "pnpm --filter @cobocho/react-naver-maps-kit test",
    "typecheck": "pnpm --filter @cobocho/react-naver-maps-kit typecheck",
    "lint": "pnpm -r lint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm install && pnpm --filter react-naver-maps-kit-docs build",
    "example:dev": "pnpm --filter example dev",
    "example:build": "pnpm --filter example build"
  },
  "keywords": [
    "react",
    "naver-maps",
    "maps",
    "monorepo"
  ],
  "author": "cobocho <contact@un-defined.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cobocho/react-naver-maps-kit.git"
  },
  "homepage": "https://github.com/cobocho/react-naver-maps-kit#readme",
  "bugs": {
    "url": "https://github.com/cobocho/react-naver-maps-kit/issues"
  },
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "prettier": "^3.5.2"
  },
  "engines": {
    "node": ">=18"
  }
}
